{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üöÄ Flutter: Web (Chrome Debug)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=8080",
        "--web-hostname=localhost",
        "--release=false"
      ],
      "env": {
        "FLUTTER_WEB_RENDERER": "canvaskit",
        "FLUTTER_WEB_CANVASKIT_URL": "https://www.gstatic.com/flutter-canvaskit/canvaskit.js"
      },
      "cwd": "${workspaceFolder}",
      "console": "debugConsole",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "üîç Flutter: Web (Debug Mode)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=8080",
        "--debug",
        "--start-paused"
      ],
      "env": {
        "FLUTTER_WEB_RENDERER": "html"
      },
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "‚ö° Flutter: Web (Profile)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--profile",
        "--web-port=8080"
      ],
      "env": {
        "FLUTTER_WEB_RENDERER": "canvaskit"
      },
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "üîß Flutter: Web DevTools",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=8080",
        "--devtools"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}